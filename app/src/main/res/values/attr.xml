<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--WheelView-->
    <declare-styleable name="WheelView">
        <attr name="wheel_float_layout" format="reference"/>
        <attr name="wheel_item_top_padding" format="dimension"/>
        <attr name="wheel_item_bottom_padding" format="dimension"/>
    </declare-styleable>

    <!--MultiplTextView-->
    <declare-styleable name="MultiplTextView">
        <attr name="runText" format="boolean"/>
        <attr name="gradient" format="boolean"/>
        <attr name="gradientStartColor" format="color"/>
        <attr name="gradientCenterColor" format="color"/>
        <attr name="gradientEndColor" format="color"/>
        <attr name="removeDefaultPadding" format="boolean"/>
        <attr name="textFont" format="string"/>
    </declare-styleable>

</resources>